        <doctext name="txt"
            text="Open Source Apache 2.0"/>
    </docsection>
    <docsection name="doc_2"
        text="Limitations">
        <doctext name="txt"
            text="Core instruction set only. Does not support MMU &amp; TLB."/>
    </docsection>
    <formalattribute name="fifos"
        type="bool">
        <docsection name="doc"
            text="Description">
            <doctext name="txt"
                text="Turn on FIFO feature"/>
        </docsection>
    </formalattribute>
    <formalattribute name="verbose"
